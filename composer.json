{
    "name": "code4interactive/view",
    "description": "Laravel View elements / addons",
    "keywords": [
        "laravel",
        "view"
    ],
    "homepage": "https://github.com/code4interactive/view",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Bartczak",
            "email": "artur.bartczak@code4.pl"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "illuminate/support" : "~5.2",
        "laravelcollective/html": "5.1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Code4\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code4\\View\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
